{
    "name": "sonata-project/classification-media-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataClassificationMediaBundle",
    "keywords": [
        "media",
        "sonata",
        "classification"
    ],
    "homepage": "https://sonata-project.org/bundles/classification-media",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Gripp",
            "email": "mail@core23.de"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataClassificationMediaBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "sonata-project/classification-bundle": "^3.6 || ^4.0",
        "sonata-project/media-bundle": "^3.10 || ^4.0",
        "symfony/config": "^2.8 || ^3.2 || ^4.0",
        "symfony/dependency-injection": "^2.8 || ^3.2 || ^4.0",
        "symfony/framework-bundle": "^2.8 || ^3.2 || ^4.0",
        "symfony/http-kernel": "^2.8 || ^3.2 || ^4.0"
    },
    "conflict": {
        "sonata-project/admin-bundle": "<3.31 || >=4.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^3.0",
        "sonata-project/admin-bundle": "^3.31",
        "symfony/phpunit-bridge": "^4.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sonata\\ClassificationMediaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\ClassificationMediaBundle\\Tests\\": "tests/"
        }
    }
}
